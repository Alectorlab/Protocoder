<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="prototypr" parent="android:Theme.Holo.Light"></style>

    <style name="MWMTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/MWMTheme.ActionBarStyle</item>
    </style>

    <style name="MWMTheme.ActionBarStyle" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:titleTextStyle">@style/MWMTheme.ActionBar.TitleTextStyle</item>
        <item name="android:background">@color/mwmblue</item>
    </style>

    <style name="MWMTheme.ActionBar.TitleTextStyle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>