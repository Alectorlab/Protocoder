<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- toolbar -->
    <style name="ToolBarAppRunner.Base" parent="">

    </style>

    <style name="ToolBarAppRunner.AppRunner" parent="ToolBarAppRunner.Base">
    </style>

    <style name="TextAppearance.Widget.Event.Toolbar.Title.AppRunner" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">

    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base"/>

    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">@color/mwmblue</item>

        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/mwmmagenta</item>

        <!-- colorAccent is used as the default value for colorControlActivated
        which is used to tint widgets -->
        <item name="colorAccent">@color/mwmorange</item>

    </style>

</resources>